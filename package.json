{
  "name": "oioio",
  "productName": "oioio",
  "version": "0.0.1",
  "description": "oioio desktop application",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start:dev": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:all": "electron-forge make --platform=all",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf out .webpack dist",
    "deps:check": "npm audit",
    "deps:fix": "npm audit fix",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "health-check": "bash scripts/health-check.sh"
  },
  "keywords": [],
  "author": {
    "name": "Nick Budzban",
    "email": "nichbud@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.27.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "36.3.1",
    "electron-notarize": "^1.2.2",
    "eslint": "^9.27.0",
    "eslint-plugin-node": "^11.1.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4"
  },
  "dependencies": {
    "electron-log": "^5.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2"
  }
}
